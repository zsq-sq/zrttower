<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="alivc_music_bg_item_selected">#00c2dd</color>
    <dimen name="alivc_record_title_icon_margin">10dp</dimen>
    <dimen name="alivc_record_title_icon_padding">5dp</dimen>
    <dimen name="aliyun_activity_horizontal_margin">16dp</dimen>
    <dimen name="aliyun_activity_vertical_margin">16dp</dimen>
    <dimen name="aliyun_effect_content_height">100dp</dimen>
    <dimen name="aliyun_fab_margin">16dp</dimen>
    <dimen name="font_size_28px">14.0sp</dimen>
    <dimen name="fout_size_36px">18sp</dimen>
    <dimen name="list_item_space">10dp</dimen>
    <string name="alivc_music_complete">OK</string>
    <string name="alivc_music_copyright">Provided by Taihe Music DMH</string>
    <string name="alivc_music_copyright_link">How to get</string>
    <string name="alivc_music_empty_music">Original</string>
    <string name="alivc_music_local_music">Local Music</string>
    <string name="alivc_music_network_not_connect">No network is found. Check your network and try again</string>
    <string name="alivc_music_no_free_memory">Insufficient memory. Please clear memory cache</string>
    <string name="alivc_music_online_music">Online Music</string>
    <string name="alivc_music_play_url_null">Url is empty</string>
    <string name="alivc_music_select_music">Music</string>
    <style name="alivc_record_btn_style" parent="@android:style/Widget.Button">
        <item name="android:background">@drawable/alivc_record_btn_bg</item>
        <item name="android:layout_width">100dp</item>
        <item name="android:textSize">@dimen/alivc_common_font_14</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:layout_height">36dp</item>
    </style>
    <style name="alivc_record_line_style" parent="@android:style/Widget.EditText">

        <item name="android:maxLines">2</item>
        <item name="android:minHeight">20dp</item>
        <item name="android:ellipsize">end</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textColorHint">@android:color/darker_gray</item>
        <item name="android:background">@drawable/alivc_record_round_bkg</item>
        <item name="android:textSize">@dimen/alivc_common_font_14</item>
    </style>
    <declare-styleable name="MusicCircleProgressBar">
        <attr format="dimension" name="musicBackgroundWidth"/>
        <attr format="dimension" name="musicBackgroundHeight"/>
        <attr format="dimension" name="musicProgressWidth"/>
        <attr format="dimension" name="musicProgressThickness"/>
        <attr format="color|reference" name="musicBackgroundColor"/>
        <attr format="color|reference" name="musicProgressColor"/>
        <attr format="color|reference" name="musicProgressThicknessColor"/>
    </declare-styleable>
</resources>