    <declare-styleable name="IndicatorSeekBar">
        //seekBar
        <attr format="float" name="isb_max"/><!-- the max value of seekBar to seek, default 100-->
        <attr format="float" name="isb_min"/><!-- the min value of seekBar to seek, default 0 -->
        <attr format="float" name="isb_progress"/><!-- the current progress value of seekBar, default 0-->
        <attr format="boolean" name="isb_clear_default_padding"/><!-- set seekBar's leftPadding&rightPadding to zero, default false, default padding is 16dp-->
        <attr format="boolean" name="isb_forbid_user_seek"/><!--prevent user from touching to seek, default false-->
        <attr format="boolean" name="isb_touch_to_seek"/><!--user change the thumb's location by touching thumb/touching track,true for touching track to seek. false for touching thumb; default true-->
        <attr format="boolean" name="isb_progress_value_float"/><!--set the value of seekBar to float type, default false-->
        <attr name="isb_seek_bar_type"><!-- the type for seekBar, default 0.-->
            <enum name="continuous" value="0"/>
            <enum name="continuous_texts_ends" value="1"/>
            <enum name="discrete_ticks" value="2"/>
            <enum name="discrete_ticks_texts" value="3"/><!--has below text-->
            <enum name="discrete_ticks_texts_ends" value="4"/><!--has below text of both ends of seekBar-->
        </attr>
        //indicator
        <attr name="isb_indicator_type"><!-- the type for indicator, default rectangle_rounded_corner/0.-->
            <enum name="rectangle_rounded_corner" value="0"/>
            <enum name="rectangle" value="1"/>
            <enum name="circular_bubble" value="2"/>
            <enum name="custom" value="3"/><!--choose custom type that you can set the custom indicator layout you want.-->
        </attr>
        <attr format="boolean" name="isb_show_indicator"/><!-- show indicator or not when seeking, default true-->
        <attr format="color|reference" name="isb_indicator_color"/><!-- indicator's color, default #FF4081-->
        <attr format="reference" name="isb_indicator_custom_layout"/><!-- when you set indicator type to custom , you can set this layout indicator layout you want-->
        <attr format="reference" name="isb_indicator_custom_top_content_layout"/> <!--set this layout indicator top view you want, no effect arrow , effect on indicator type : rectangle or rectangle_rounded_corner-->
        <attr format="color|reference" name="isb_indicator_text_color"/><!-- indicator's text color, default #FF4081 , work on indicator type : rectangle or rectangle_rounded_corner-->
        <attr format="dimension|reference" name="isb_indicator_text_size"/><!-- indicator's text size, default 13sp,  work on indicator type : rectangle or rectangle_rounded_corner-->
        <attr format="boolean" name="isb_indicator_stay"/><!-- indicator is visible when SeekBar visible, gone when SeekBar gone-->
        //track
        <attr format="dimension|reference" name="isb_track_background_bar_size"/><!-- set indicatorSeekBar's track background bar size, default 2dp-->
        <attr format="color|reference" name="isb_track_background_bar_color"/><!-- set indicatorSeekBar's track background bar color, default #D7D7D7-->
        <attr format="dimension|reference" name="isb_track_progress_bar_size"/><!-- set indicatorSeekBar's track progress bar size, default 2dp-->
        <attr format="color|reference" name="isb_track_progress_bar_color"/><!-- set indicatorSeekBar's track progress bar color, default #FF4081-->
        <attr format="boolean" name="isb_track_rounded_corners"/><!-- set indicatorSeekBar's track's both ends's corners to rounded/square, default false-->
        //thumb
        <attr format="boolean" name="isb_thumb_progress_stay"/><!-- set thumb below text to stay after seek, default false, work on seekBar type : continuous / discrete_ticks-->
        <attr format="color|reference" name="isb_thumb_color"/><!--set thumb's color, default #FF4081-->
        <attr format="dimension|reference" name="isb_thumb_width"/><!--set thumb's size, default 14dp, thumb size will be limited in 30dp-->
        <attr format="reference" name="isb_thumb_drawable"/><!--set custom thumb's drawable you want,thumb size will be limited in 30dp, if drawable less than 30dp ,will show in raw size -->
        //tick
        <attr format="reference" name="isb_tick_drawable"/><!--set custom tick's drawable you want, custom drawable will be limited in 30dp, if less than 30dp ,will show in raw size-->
        <attr format="color|reference" name="isb_tick_color"/><!--set tick's color, default #FF4081-->
        <attr format="integer" name="isb_tick_num"/><!--seekBar's tick count, default 5-->
        <attr format="dimension|reference" name="isb_tick_size"/><!--set the tick width, default 10dp,custom drawable will be limited in 30dp, if less than 30dp ,will show in raw size-->
        <attr format="boolean" name="isb_tick_both_end_hide"/><!--hide 2 ticks on the seekBar's both ends, default false-->
        <attr format="boolean" name="isb_tick_on_thumb_left_hide"/><!--hide the ticks on the seekBar's thumb left, default false-->
        <attr name="isb_tick_type"><!--select the tick shape type, default rectangle/1-->
            <enum name="none" value="0"/>
            <enum name="rec" value="1"/>
            <enum name="oval" value="2"/>
        </attr>
        //text
        <attr format="color|reference" name="isb_text_color"/><!--set the color of text below tick, default #FF4081-->
        <attr format="string|reference" name="isb_text_left_end"/><!--set the text below seekBar left end, default min value string, work on seekBar type :CONTINUOUS_TEXTS_ENDS/DISCRETE_TICKS_TEXTS/DISCRETE_TICKS_TEXTS_ENDS-->
        <attr format="string|reference" name="isb_text_right_end"/><!--set the text below seekBar right end, default max value string, work on seekBar type :CONTINUOUS_TEXTS_ENDS/DISCRETE_TICKS_TEXTS/DISCRETE_TICKS_TEXTS_ENDS-->
        <attr format="dimension|reference" name="isb_text_size"/><!--set the text size of tick below text, default 13sp-->
        <attr format="reference" name="isb_text_array"/><!--set the texts below tick to replace default progress text, default string of progress, work on seekBar type :DISCRETE_TICKS_TEXTS-->
        <attr name="isb_text_typeface"><!--select the text typeface, default normal-->
            <enum name="normal" value="0"/>
            <enum name="monospace" value="1"/>
            <enum name="sans" value="2"/>
            <enum name="serif" value="3"/>
        </attr>
    </declare-styleable>
